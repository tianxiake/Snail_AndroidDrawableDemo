<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.nebulaera.myandroidtestdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/MyCustomAppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".AnimationDrawableActivity"
            android:label="AnimationDrawableActivity">
        </activity>
        <activity
            android:name=".PropertyAnimatorActivity"
            android:label="PropertyAnimatorActivity">
        </activity>
        <activity
            android:name=".RawXmlActivity"
            android:label="RawXmlActivity">
        </activity>
        <activity
            android:name=".CustomAttributeActivity"
            android:label="CustomAttributeActivity">
        </activity>
        <activity android:name=".UseRawSourceActivity">
        </activity>
    </application>

</manifest>